#include <stdio.h>
#include <bitset>
using namespace std;
const int n = int(86028125);
bitset<n+1> p;
int primes[5000102], k;
/*
void fastscan(int &x)
    {
        bool neg=false;
        register int c;
        x =0;
        c=getchar();
        if(c=='-')
        {
            neg = true;
            c=getchar();
